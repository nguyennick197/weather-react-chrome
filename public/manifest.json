{
  "name": "Luna Weather Assistant",
  "description": "Weather app Chrome extension built in React",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Luna Weather Assistant"
  },
  "icons": {
    "16": "night96.png",
    "48": "night96.png",
    "128": "night96.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "geolocation"
  ]
}
